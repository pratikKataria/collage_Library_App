<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape
        android:shape="line">
        <stroke
            android:dashWidth="15dp"
            android:width="5dp"
            android:dashGap="10dp"
            android:color="#FF8800"/>
    </shape>
</rotate>
